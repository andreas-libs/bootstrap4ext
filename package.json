{
  "name": "bootstrap4ext",
  "version": "1.0.1",
  "description": "Extend the bootstrap 4 library",
  "main": "dist/bootstrap-ext.css",
  "browser": "index.html",
  "author": "Andreas Ntrouflias <antrouflias@gmail.com>",
  "scripts": {
    "sass-watch": "sass --watch sass/bootstrap-ext.scss dist/bootstrap-ext.css",
    "sass-build": "sass sass/bootstrap-ext.scss dist/bootstrap-ext.css && sass sass/bootstrap-ext.scss dist/bootstrap-ext.min.css --style compressed"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.0",
    "bootstrap": "^4.4.1",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.0"
  },
  "devDependencies": {
    "sass": "^1.24.4"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lib-hub/bootstrap4ext"
  }
}
